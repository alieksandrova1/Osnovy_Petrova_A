#include <stdio.h>
#include <stdlib.h>

int main() {
    FILE *file;
    file = fopen("text.txt", "a");
    fputs("\n", file);
    fclose(file);
    file = fopen("text.txt", "r");
    char buff[1000];
    char *pr;
    int max = -99999;
    int maxIndex = -1;
    int line = 0;
    int flag = 0;

    while(1) {
        pr = fgets(buff, 1000, file);
        if (pr == NULL) {
            break;
        }
        int i = 0;
        char curr[1000] = "";
        int k = 0;
        while (buff[i] != '\0') {
            if (((buff[i] >= '0') && (buff[i] <= '9'))|| buff[i] == '-') {
                if(flag == 0)
                {
                    curr[k] = buff[i];
                    k++;
                }

                if((buff[i-1] >= 'a') && (buff[i-1] <= 'z')||(buff[i+1] >= 'a') && (buff[i+1] <= 'z'))
                {
                    k = 0;
                    flag = 1;
                }

            } else {
                if (k != 0) {
                    curr[k] = '\n';
                    int nn = atoi(curr);
                    if (nn > max) {
                        max = nn;
                        maxIndex = line;
                    }
                }
                k = 0;
            }
            if((buff[i+1] == ' ')||(buff[i+1] == '\n')||(buff[i+1] == '.')||(buff[i+1] == ','))
            {
                flag = 0;
            }
            i++;
        }
        line++;
    }

    fclose(file);
    file = fopen("text.txt", "r");
    FILE *out;
    out = fopen("out.txt", "w");
    line = 0;
    while (1) {
        pr = fgets(buff, 1000, file);
        if (pr == NULL) {
            break;
        }

        if (line == maxIndex) {
            line++;
            continue;
        }

        line++;
        fputs(buff, out);

    }
    fclose(file);
    fclose(out);
    return 0;
}


